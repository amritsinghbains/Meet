<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context=".selectvehicle">

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/car_black"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:src="@drawable/car1"
        android:onClick="car1_tap" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/car_white"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/car_black"
        android:layout_toEndOf="@+id/car_black"
        android:src="@drawable/car2"
        android:onClick="car2_tap" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/car_red"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/car_white"
        android:layout_toEndOf="@+id/car_white"
        android:src="@drawable/car3"
        android:onClick="car3_tap" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/car_yellow"
        android:layout_toEndOf="@+id/car_red"
        android:layout_toRightOf="@+id/car_red"
        android:src="@drawable/car4"
        android:onClick="car4_tap"
        android:layout_below="@+id/car_black"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="22dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/car_blue"
        android:layout_toEndOf="@+id/car_yellow"
        android:src="@drawable/truck1"
        android:onClick="truck1_tap"
        android:layout_toStartOf="@+id/car_red"
        android:layout_toLeftOf="@+id/car_red"
        android:layout_alignTop="@+id/car_yellow"
        android:layout_toRightOf="@+id/car_yellow" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/car_mauve"
        android:layout_toEndOf="@+id/car_blue"
        android:src="@drawable/bike1"
        android:onClick="bike1_tap"
        android:layout_alignTop="@+id/car_blue"
        android:layout_toRightOf="@+id/car_blue" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/car_orange"
        android:layout_toEndOf="@+id/car_mauve"
        android:layout_toRightOf="@+id/car_mauve"
        android:src="@drawable/bike2"
        android:onClick="bike2_tap"
        android:layout_below="@+id/car_yellow"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="21dp" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton"
        android:layout_toEndOf="@+id/car_orange"
        android:src="@drawable/bike3"
        android:onClick="bike3_tap"
        android:layout_alignTop="@+id/car_orange"
        android:layout_toRightOf="@+id/car_orange" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton2"
        android:layout_toEndOf="@+id/imageButton"
        android:src="@drawable/plane1"
        android:onClick="plane1_tap"
        android:layout_alignTop="@+id/imageButton"
        android:layout_alignRight="@+id/car_mauve"
        android:layout_alignEnd="@+id/car_mauve"
        android:layout_toRightOf="@+id/imageButton" />

    <ImageButton
        android:layout_width="100dp"
        android:layout_height="100dp"
        android:id="@+id/imageButton3"
        android:layout_toEndOf="@+id/imageButton"
        android:layout_toRightOf="@+id/imageButton"
        android:src="@drawable/plane2"
        android:onClick="plane2_tap"
        android:layout_below="@+id/car_orange"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="21dp" />

</RelativeLayout>
